{{#sprites}}
.icon-{{name}}::before {
  display: inline-block;
  background-image: url({{{escaped_image}}});
  background-position: {{px.offset_x}} {{px.offset_y}};
  width: {{px.width}};
  height: {{px.height}};
  margin-right: 5px;
  vertical-align: middle;
  content:"";
}
.title-{{name}} {
}
.title-{{name}} h2 {
	  font-size:{{px.height}};
}
.title-{{name}} h2::before, .title-{{name}} h5::before {
	display: inline-block;
	background-image: url({{{escaped_image}}});
	background-position: {{px.offset_x}} {{px.offset_y}};
	width: {{px.width}};
	height: {{px.height}};
	margin-right: 10px;
	vertical-align: middle;
	content:"";
	position: relative;
}
{{/sprites}}